<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dunchi.android_view">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".general.AbsoluteLayout"></activity>
        <activity android:name=".general.TableLayoutActivity" />
        <activity android:name=".general.MotionLayoutActivity" />
        <activity android:name=".general.ConstraintLayoutActivity" />
        <activity android:name=".general.FrameLayoutActivity" />
        <activity android:name=".general.RelativeLayoutActivity" />
        <activity android:name=".general.LinearLayoutActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>